type Bool = 
  | False
  | True

module nat {
  type Nat =
    | Zero
    | Succ(Nat)
}

impl Nat {
  def is_zero(self : Nat) : Bool => match self {
    nat::Nat::Zero => return True,
    nat::Nat::Succ(_) => return False
  }

  def is_succ(self : Nat) : Bool => match self {
    self::nat::Nat::Zero => return False,
    self::nat::Nat::Succ(_) => return True
  }
}